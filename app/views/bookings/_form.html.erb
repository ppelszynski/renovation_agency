
<%= f.input :company_id, collection: Company.all, autofocus: true %>
<%= f.input :building_location_id, collection: BuildingLocation.all, label_method: :adress, value_method: :id %>
<%= f.input :date_from, as: :date %>
<%= f.input :date_to, as: :date %>

<%= f.submit "#{action} booking", class: "btn btn-outline-dark" %>